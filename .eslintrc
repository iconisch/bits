{
  "extends": [
    "standard",
    "standard-react"
  ],
  "rules": {
    "jsx-quotes": ["error", "prefer-double"],
    "no-extend-native": "off",
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 0 }],
    "no-new": "off",
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", "never"]
  }
}
